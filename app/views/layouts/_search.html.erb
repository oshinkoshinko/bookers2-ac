<% if user_signed_in? %>
 <div class="search-form">
   <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :search %>
    <%= f.select(:option, options_for_select([["user", "1"],["book", "2"]])) %>
    <%= f.select(:choice, options_for_select([["完全一致", "1"],["前方一致", "2"],["後方一致", "3"],["部分一致", "4"]])) %>
    <%= f.submit '検索', name: nil %>
    <% end %>
 </div>
 <% end %>